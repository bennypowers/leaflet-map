{
  "name": "leaflet-element",
  "version": "0.0.15",
  "main": "index.js",
  "module": "index.js",
  "license": "ISC",
  "scripts": {
    "prepublishOnly": "tsc",
    "start": "concurrently --kill-others --names tsc,serve \"npm run tsc:watch\" \"npm run serve\"",
    "serve": "es-dev-server --node-resolve --watch --open",
    "docs": "wca --outFile custom-elements.json",
    "lint:eslint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "tsc:watch": "tsc -w",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "test": "tsc && karma start --coverage",
    "test:watch": "concurrently --kill-others --names tsc,karma \"npm run tsc:watch\" \"karma start --auto-watch=true --single-run=false\""
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^3.0.0",
    "@open-wc/testing": "^2.5.25",
    "@open-wc/testing-karma": "^4.0.4",
    "@pwrs/eslint-config": "0.0.9",
    "@types/leaflet": "^1.5.17",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "api-viewer-element": "^0.4.4",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.57.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "geojson": "^0.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "patch-package": "^6.2.2",
    "typescript": "^3.9.5",
    "web-component-analyzer": "^1.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@open-wc/dedupe-mixin": "^1.3.0",
    "@pwrs/mixins": "0.0.9",
    "leaflet": "^1.6.0",
    "lit-element": "^2.4.0",
    "resize-observer-polyfill": "^1.5.1"
  }
}
